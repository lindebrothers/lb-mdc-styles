{
  "name": "lb-mdc-styles",
  "version": "0.0.1",
  "description": "MATERIAL COMPONENTS FOR THE WEB",
  "main": "index.js",
  "scripts": {
    "testcss": "postcss bakery/base.css > bakery/output.css",
    "clean": "echo \"Cleaning completed\" $([ -d ./dist ] && rm -rf ./dist $([ -d ./build ] && rm -rf ./build $([ -d ./public/webpack ] && rm -rf ./public/webpack)))",
    "start:snowpack": "NODE_ENV=development snowpack dev --polyfill-node",
    "start:webpack": "npm run compile:dev:watch",
    "start": "npm run start:snowpack",
    "update:envs": "scripts/updateEnvs.sh",
    "build:snowpack": "NODE_ENV=development snowpack build --polyfill-node",
    "build:gh-pages": "BUILD_TYPE=gh-pages NODE_ENV=production webpack --config ./webpack.config.js && npm run build:snowpack",
    "test:gh-pages": "BASE_URL=/lb-mdc-styles node test-server.js",
    "print:version": "echo public/webpack/v$(./scripts/read-version-from-package.sh) | sed -e \"s/\\./-/g\"",
    "compile:prod": "NODE_ENV=production webpack --config ./webpack.config.js",
    "compile:dev": "NODE_ENV=development webpack --config ./webpack.config.js",
    "compile:dev:watch": "NODE_ENV=development webpack --config ./webpack.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:akejolin/lb-mdc-styles.git"
  },
  "author": "",
  "license": "ISC",
